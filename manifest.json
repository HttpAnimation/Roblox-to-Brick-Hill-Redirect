{
  "manifest_version": 2,
  "name": "Roblox to Brick Hill Redirect",
  "version": "1.0",
  "description": "Redirects roblox.com to https://www.brick-hill.com/dashboard",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*/*"
  ]
}
